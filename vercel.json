{
  "version": 2,
  "crons": [
    {
      "path": "/api/export-to-sheets",
      "schedule": "0 0 * * *"
    }
  ],
  "routes": [
    {
      "src": "/api/inbound-email",
      "dest": "/api/inbound-email.js"
    },
    {
      "src": "/api/emails/tenders",
      "dest": "/api/inbound-email.js"
    },
    {
      "src": "/api/emails",
      "dest": "/api/inbound-email.js"
    }
  ]
}
